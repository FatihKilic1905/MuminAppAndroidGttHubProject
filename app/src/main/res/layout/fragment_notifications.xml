<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.notifications.NotificationsFragment"
    tools:layout_editor_absoluteX="13dp"
    tools:layout_editor_absoluteY="0dp">

    <Button
        android:id="@+id/ayarlarButton"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:onClick="gecisyap"
        android:text="Ayarlar"

        tools:ignore="TouchTargetSizeCheck"
        tools:layout_editor_absoluteX="243dp"
        tools:layout_editor_absoluteY="392dp" />

    <Button
        android:id="@+id/ZekatmatikButton"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:text="Zekatmatik"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="232dp" />

    <Button
        android:id="@+id/canliButton"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:text="Kabe Canlı"
        tools:layout_editor_absoluteX="243dp"
        tools:layout_editor_absoluteY="232dp" />

    <Button
        android:id="@+id/zikirMatikButton"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:text="Online Zikirmatik"
        tools:layout_editor_absoluteX="243dp"
        tools:layout_editor_absoluteY="56dp" />

    <Button
        android:id="@+id/hatimTakipButton"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:text="Hatim Takip"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="56dp" />

    <Button
        android:id="@+id/usersButton"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:text="Kullanıcı"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="392dp" />


</androidx.constraintlayout.widget.ConstraintLayout>