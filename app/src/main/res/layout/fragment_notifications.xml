<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.notifications.NotificationsFragment"
    tools:layout_editor_absoluteX="13dp"
    tools:layout_editor_absoluteY="0dp">

    <ImageButton
        android:id="@+id/ayarlarButton"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:background="@drawable/ayarlar_buttonxxxhdpi"
        android:onClick="gecisyap"
        android:text="Ayarlar"
        android:layout_marginBottom="80dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/guideline3"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/usersButton"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:background="@drawable/kullanici_buttonxxxhdpi"
        android:text="Kullanıcı"
        app:layout_constraintBottom_toTopOf="@+id/ayarlarButton"
        app:layout_constraintEnd_toStartOf="@+id/guideline3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/zikirMatikButton" />

    <ImageButton
        android:id="@+id/ZekatmatikButton"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:background="@drawable/zekatmatik_androidxxxhdpi"
        android:text="Zekatmatik"
        android:layout_marginTop="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/guideline3"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/canliButton"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:background="@drawable/kabe_canli_buttonxxxhdpi"
        android:text="Kabe Canlı"
        app:layout_constraintBottom_toTopOf="@+id/ayarlarButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/guideline3"
        app:layout_constraintTop_toBottomOf="@+id/ZekatmatikButton" />

    <ImageButton
        android:id="@+id/zikirMatikButton"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:background="@drawable/zikirmatik_xxxhdpi"
        android:layout_marginTop="40dp"
        android:text="Online Zikirmatik"
        app:layout_constraintEnd_toStartOf="@+id/guideline3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="205dp" />


</androidx.constraintlayout.widget.ConstraintLayout>